{
  "name": "eva-express",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "postinstall": "yarn clean && yarn locale && tsc --sourceMap false && yarn copyfiles",
    "build": "yarn clean && tsc",
    "start": "NODE_PATH=dist/ nf start",
    "dev": "NODE_PATH=dist/ tsc-watch --onSuccess \"nf --env env/.env.development start web=1\"",
    "worker": "NODE_PATH=dist/ nodemon --exec \"nf --env ./env/.env.development start worker=1\"",
    "cron": "NODE_PATH=dist/ nodemon --exec \"nf --env ./env/.env.development start clock=1\"",
    "redis-cli": "./redis/src/redis-cli",
    "redis:start": "./redis/src/redis-server",
    "redis:stop": "yarn redis-cli shutdown",
    "redis:flush": "yarn redis-cli flushall",
    "migration": "drizzle-kit generate:pg",
    "clean": "rimraf dist/",
    "locale": "ts-node script/locale.ts",
    "copyfiles": "copyfiles -u 1 src/**/*.json dist/",
    "prepare": "husky install"
  },
  "dependencies": {
    "@faire/mjml-react": "^3.1.2",
    "@socket.io/redis-adapter": "^8.1.0",
    "bcrypt": "^5.1.0",
    "bullmq": "^3.10.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^2.2.0",
    "drizzle-orm": "^0.22.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "heroku-ssl-redirect": "^0.1.1",
    "html-to-text": "^9.0.4",
    "i18n": "^0.15.1",
    "ioredis": "^5.3.1",
    "jsonwebtoken": "^9.0.0",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.21",
    "mjml": "^4.13.0",
    "moment-timezone": "^0.5.41",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.1",
    "pg": "^8.10.0",
    "rate-limit-redis": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redis": "^4.6.5",
    "socket.io": "^4.6.1",
    "throng": "^5.0.0",
    "twilio": "^4.8.0",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.17",
    "@types/html-to-text": "^9.0.0",
    "@types/i18n": "^0.13.6",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/mjml": "^4.7.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.14.6",
    "@types/nodemailer": "^6.4.7",
    "@types/pg": "^8.6.6",
    "@types/react": "^18.0.28",
    "@types/throng": "^5.0.4",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.3",
    "drizzle-kit": "^0.17.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.35.0",
    "foreman": "^3.0.1",
    "husky": "^8.0.0",
    "jest": "^29.4.3",
    "jest-extended": "^3.2.4",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "rimraf": "^4.2.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.5"
  }
}
