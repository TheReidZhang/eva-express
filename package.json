{
  "name": "eva-express",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "postinstall": "yarn clean && yarn locale && tsc --sourceMap false && yarn copyfiles",
    "build": "yarn clean && tsc",
    "start": "NODE_PATH=dist/ nf start",
    "dev": "NODE_PATH=dist/ tsc-watch --onSuccess \"nf --env env/.env.development start web=1\"",
    "redis": "./redis/src/redis-server",
    "redis-stop": "./redis/src/redis-cli shutdown",
    "typeorm": "TYPEORM_CLI=true typeorm-ts-node-commonjs",
    "migrate": "NODE_PATH=src/ env-cmd -f env/.env.development yarn typeorm migration:run --dataSource src/orm/index.ts --transaction each",
    "mg": "yarn migrate",
    "rollback": "NODE_PATH=src/ env-cmd -f env/.env.development yarn typeorm migration:revert --dataSource src/orm/index.ts --transaction each",
    "rb": "yarn rollback",
    "migration": "sh -c 'NODE_PATH=src/ env-cmd -f env/.env.development yarn typeorm migration:generate src/migration/${0} -d src/orm/index.ts'",
    "clean": "rimraf dist/",
    "locale": "ts-node script/locale.ts",
    "copyfiles": "copyfiles -u 1 src/**/*.json dist/",
    "prepare": "husky install"
  },
  "dependencies": {
    "@faire/mjml-react": "^3.1.2",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/i18n": "^0.13.6",
    "@types/throng": "^5.0.4",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.4",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "heroku-ssl-redirect": "^0.1.1",
    "html-to-text": "^9.0.4",
    "i18n": "^0.15.1",
    "jsonwebtoken": "^9.0.0",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.21",
    "mjml": "^4.13.0",
    "moment-timezone": "^0.5.41",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.1",
    "pg": "^8.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "throng": "^5.0.0",
    "typeorm": "^0.3.12",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/html-to-text": "^9.0.0",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/mjml": "^4.7.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.14.6",
    "@types/nodemailer": "^6.4.7",
    "@types/react": "^18.0.28",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.35.0",
    "foreman": "^3.0.1",
    "husky": "^8.0.0",
    "jest": "^29.4.3",
    "jest-extended": "^3.2.4",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "rimraf": "^4.2.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.5"
  }
}
